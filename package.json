{
  "name": "oncheckin-app",
  "version": "1.0.0",
  "description": "Membership and event management software for Hash House Harriers.",
  "main": "index.js",
  "scripts": {
    "build": "snowpack build && npm run workbox",
    "start": "snowpack dev & npm run start-pub",
    "start-pub": "earthstar-pub --discoverable --sqlite --dataFolder ./data",
    "test": "standard",
    "workbox": "workbox generateSW workbox-config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/basham/oncheckin-gundb.git"
  },
  "author": "Chris Basham <chris@bash.am>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/basham/oncheckin-gundb/issues"
  },
  "homepage": "https://github.com/basham/oncheckin-gundb#readme",
  "devDependencies": {
    "@snowpack/plugin-svelte": "3.7.0",
    "earthstar-pub": "6.0.5",
    "snowpack": "3.8.8",
    "standard": "16.0.3",
    "workbox-cli": "6.3.0"
  },
  "dependencies": {
    "cuid": "2.1.8",
    "date-fns": "2.24.0",
    "earthstar": "6.8.5",
    "svelte": "3.42.6",
    "workbox-window": "6.3.0"
  },
  "snowpack": {
    "mount": {
      "src": "/"
    },
    "optimize": {
      "minify": true
    },
    "plugins": [
      "@snowpack/plugin-svelte"
    ]
  }
}
