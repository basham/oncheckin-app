{
  "name": "oncheckin-app",
  "version": "1.0.0",
  "description": "Membership and event management software for Hash House Harriers.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "snowpack build && npm run workbox",
    "start": "snowpack dev",
    "start-reload": "snowpack dev --reload",
    "test": "standard",
    "workbox": "workbox generateSW workbox-config.cjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/basham/oncheckin-app.git"
  },
  "author": "Chris Basham <chris@bash.am>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/basham/oncheckin-app/issues"
  },
  "homepage": "https://github.com/basham/oncheckin-app#readme",
  "devDependencies": {
    "@snowpack/plugin-svelte": "3.7.0",
    "snowpack": "3.8.8",
    "standard": "17.0.0",
    "workbox-cli": "6.5.3"
  },
  "dependencies": {
    "cuid": "2.1.8",
    "date-fns": "2.28.0",
    "earthstar-bundle": "basham/earthstar-bundle#v9.3.2",
    "qrious": "4.0.2",
    "svelte": "3.48.0",
    "workbox-window": "6.5.3"
  },
  "snowpack": {
    "alias": {
      "@src": "./src"
    },
    "mount": {
      "src": "/"
    },
    "optimize": {
      "minify": true
    },
    "plugins": [
      "@snowpack/plugin-svelte"
    ]
  }
}
